{
  "name": "frontend",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@emotion/react": "^11.13.3",
    "@emotion/styled": "^11.13.0",
    "@mui/icons-material": "^6.1.6",
    "@mui/material": "^6.1.6",
    "@radix-ui/react-alert-dialog": "^1.1.2",
    "@radix-ui/react-slot": "^1.1.0",
    "axios": "^1.7.9",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "eslint": "^8.57.1",
    "jwt-decode": "^4.0.0",
    "lucide-react": "^0.454.0",
    "prismjs": "^1.29.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-markdown": "^9.0.1",
    "react-phone-input-2": "^2.15.1",
    "react-phone-number-input": "^3.4.9",
    "react-router-dom": "^6.27.0",
    "react-scripts": "5.0.1",
    "react-syntax-highlighter": "^15.6.1",
    "remark-gfm": "^4.0.0",
    "tailwind-merge": "^2.5.4",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "jest-coverage": "jest --coverage"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/plugin-proposal-private-property-in-object": "^7.18.6",
    "@shadcn/ui": "^0.0.4",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.0.1",
    "@testing-library/user-event": "^14.5.2",
    "axios-mock-adapter": "^2.1.0",
    "jest": "^27.5.1"
  },
  "jest": {
    "testEnvironment": "jsdom",
    "moduleNameMapper": {
      "\\.(css|less|scss|sass)$": "<rootDir>/jest.cssMock.js"
    },
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/App.js",
      "src/index.js",
      "src/Navbar.js",
      "src/NewSummary.js",
      "src/SelectedSummary.js",
      "src/SummaryToggle.js",
      "src/AuthForms.js",
      "src/Dashboard.js",
      "src/SummariesList.js"
    ],
    "coverageDirectory": "coverage",
    "testMatch": [
      "**/__tests__/Navbar.test.js",
      "**/__tests__/NewSummary.test.js",
      "**/__tests__/SelectedSummary.test.js",
      "**/__tests__/Dashboard.test.js",
      "**/__tests__/AuthForms.test.js",
      "**/__tests__/MarkdownRenderer.test.js",
      "**/__tests__/index.test.js",
      "**/__tests__/App.test.js",
      "**/__tests__/SummaryToggle.test.js",
      "**/__tests__/AuthForms.test.js",
      "**/__tests__/SummariesList.test.js"
    ],
    "coverageReporters": [
      "text-summary"
    ]
  }
}
